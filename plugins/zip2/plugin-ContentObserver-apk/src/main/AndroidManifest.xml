<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="scut.carson_ho.contentprovider2">
<!--    <uses-permission android:name="scut.carson_ho.Read"/>-->
<!--    <uses-permission android:name="scut.carson_ho.Write"/>-->
    <uses-permission android:name="scut.carson_ho.PROVIDER"/>



    <queries>
        <package android:name="com.nolovr.shadow.dev.host" />
        <package android:name="scut.carson_ho.contentprovider" />
        <package android:name="scut.carson_ho.contentprovider2" />
        <provider android:authorities="cn.scu.myprovider/user" />
